<div class="jumbotron">
   <h1 class="text-center">Pedido</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Nome Vendedor</th>
      <th>Nome Produto</th>
      <th>Preço</th>
      <th>Ações</th>
    </tr>
  </thead>
  <tbody>
    <tr ng-repeat="preco in precosEmpresa">
      <td>{{preco.Vendedor.nome}}</td>
      <td>{{preco.Produto.nome}}</td>
      <td>{{preco.preco}}</td>
      <td>
        <button type="button" class="btn btn-primary">Atualizar</button>
        <button type="button" class="btn btn-danger">Apagar</button>
      </td>
    </tr>
  </tbody>
</table>
<div class="col-md-6">
  <div class="form-group">
    <button type="button" class="btn btn-primary">Incluir</button>
    <a href="/#/" class="btn btn-default">Voltar</a>
  </div>
</div>
